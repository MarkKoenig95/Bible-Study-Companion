{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "configurations": {
    "ios.sim.release": {
      "binaryPath": "ios/build/BibleStudyCompanion/Build/Products/Release-iphoneos/BibleStudyCompanion.app",
      "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/BibleStudyCompanion.xcworkspace -configuration Release -scheme BibleStudyCompanion",
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 11"
      }
    },
    "ios.sim.debug": {
      "binaryPath": "ios/build/BibleStudyCompanion/Build/Products/Debug-iphoneos/BibleStudyCompanion.app",
      "build": "xcodebuild -workspace ios/BibleStudyCompanion.xcworkspace -configuration Debug -scheme BibleStudyCompanion",
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 11"
      }
    },
    "ios.none": {
      "binaryPath": "ios/build/BibleStudyCompanion/Build/Products/Debug-iphoneos/BibleStudyCompanion.app",
      "build": "xcodebuild -workspace ios/BibleStudyCompanion.xcworkspace -configuration Debug -scheme BibleStudyCompanion ",
      "type": "ios.none",
      "device": {
        "type": "iPhone 11"
      },
      "session": {
        "server": "ws://localhost:8099",
        "sessionId": "com.wix.demo.react.native"
      }
    },
    "android.emu.debug": {
      "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
      "build": "cd android && ./gradlew app:assembleDebug  app:assembleAndroidTest -DtestBuildType=debug --stacktrace && cd ..",
      "type": "android.emulator",
      "device": {
        "avdName": "Nexus_5X_API_29_x86"
      }
    },
    "android.emu.release": {
      "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
      "build": "cd android && ./gradlew  app:assembleRelease  app:assembleAndroidTest -DtestBuildType=release --scan && cd ..",
      "type": "android.emulator",
      "device": {
        "avdName": "Nexus_5X_API_29_x86"
      }
    }
  }
}
