{
  "name": "bible-study-companion",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "android-fetch": "adb -s emulator-5554 shell cmd jobscheduler run -f com.biblestudycompanion 999",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "cd android && ./gradlew bundleRelease",
    "clean": "react-native-clean-project",
    "clean-android": "cd android && ./gradlew clean",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "update-version": "react-native-version --never-amend"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^3.0.1",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/picker": "^1.6.6",
    "@react-native-community/push-notification-ios": "^1.4.1",
    "@react-navigation/bottom-tabs": "^5.5.2",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "i18n-js": "^3.7.0",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.27.0",
    "react": "16.9.0",
    "react-native": "^0.62.2",
    "react-native-background-fetch": "^3.1.0",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-localize": "^1.4.0",
    "react-native-push-notification": "^5.0.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-native-searchable-dropdown": "^1.1.1",
    "react-native-sqlite-storage": "^4.1.0",
    "react-native-vector-icons": "^6.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "24.9.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.56.4",
    "react-native-clean-project": "^3.3.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
